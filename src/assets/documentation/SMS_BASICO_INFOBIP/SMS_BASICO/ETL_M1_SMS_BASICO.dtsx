         *  string filePath = (string)rawConnection;
         *  //Use the connection in some code here, then release the connection
         *  Dts.Connections["Prices.zip"].ReleaseConnection(rawConnection);
         * */
        #endregion


        /// <summary>
        /// This method is called when this script task executes in the control flow.
        /// Before returning from this method, set the value of Dts.TaskResult to indicate success or failure.
        /// To open Help, press F1.
        /// </summary>

        /*
            Cuando se ejecuta el scrip task llama al metodo 
            Main().
			
            El valor asigando a la variable url de C# proviene de 
            convertir la variable v_url del paquete de ETL. Adicionalmente
            se define un iterador entero llamado i.
        */
        public void Main()
        {
            // TODO: Add your code here



            String url = Dts.Variables["User::v_url"].Value.ToString();
            String v_sms = Dts.Variables["User::v_sms"].Value.ToString();
            String texto;
            String textoXml;
            String v_to = "";
            String v_groupId = "";
            String v_groupName = "";
            String v_id = "";
            String v_name = "";
            String v_description = "";
            String v_smsCount = "";
            String v_messageid = "";
            String salida = "";
            int i = 0;



            WebRequest request = HttpWebRequest.Create(url); // Realiza una solicitud a la url especificada en el parametro url
            WebResponse response = request.GetResponse(); // Obtiene una respuesta del servidor.
            StreamReader reader = new StreamReader(response.GetResponseStream());

            String urlText = reader.ReadToEnd(); // Lee todo lo que se encuentre en la respuesta que es un xml

            /* Respuesta
                 <?xml version='1.0' encoding='UTF-8'?>
                 <smsResponse>
                     <messages>
                         <message>
                             <to>573196966720</to>
                             <status>
                                 <groupId>1</groupId>
                                 <groupName>PENDING</groupName>
                                 <id>7</id>
                                 <name>PENDING_ENROUTE</name>
                                 <description>Message sent to next instance</description>
                             </status>
                             <smsCount>1</smsCount>
                             <messageId>2453161806341630140</messageId>
                         </message>
                     </messages>
                 </smsResponse>
           
             */
            string xml = urlText.Replace("<?xml version='1.0' encoding='utf-8'?>", ""); // Le quita 


            // Escribe en el archivo F:\\ProyectosIntegracion\\SMS\\salida_sms_basico\\salidaxml.xml


            using (StreamWriter sw = File.AppendText("F:\\ProyectosIntegracion\\SMS\\salida_sms_basico_infobip\\salidaXmlInfobip.xml"))
                sw.WriteLine(urlText); // Escribe todas las lineas que vienen en el xml


            // Lee el contenido del archivo XML
            XmlTextReader textReader = new XmlTextReader("F:\\ProyectosIntegracion\\SMS\\salida_sms_basico_infobip\\salidaXmlInfobip.xml");
            textReader.Read();

            // Mientras hayan datos en el archivo lo lee       
            while (textReader.Read() && textReader.Value.ToString() != "\r\n")
            {
                XmlNodeType nType = textReader.NodeType;   //  Identifica el tipo de nodo del xml 

                if (nType == XmlNodeType.Text) //Si el nodo es de tipo texto
                {

                    i = i + 1; // Suma 1 al contador para identificar el orden la etiqueta 
                    /*
                     *  En la estructura del XML, la primer etiqueta en aparecer es to 
                     *  por ende si i =1 entonces el textoXml correspondera a la variable v_to 
                     *
                     */

                    texto = textReader.Value.ToString() + "|";
                    textoXml = textReader.Value.ToString(); // Almacena el valor de la etiqueta
                    salida += texto;
                    if (i == 1) { v_to = textoXml; }
                    if (i == 2) { v_groupId = textoXml; }
                    if (i == 3) { v_groupName = textoXml; }
                    if (i == 4) { v_id = textoXml; }
                    if (i == 5) { v_name = textoXml; }
                    if (i == 6) { v_description = textoXml; }
                    if (i == 7) { v_smsCount = textoXml; }
                    if (i == 8) { v_messageid = textoXml; }
                }
            }

            /* 
             * Asigna las variables al paquete de la ETL
             */
            Dts.Variables["v_to"].Value = v_to;
            Dts.Variables["v_groupId"].Value = v_groupId;
            Dts.Variables["v_groupName"].Value = v_groupName;
            Dts.Variables["v_id"].Value = v_id;
            Dts.Variables["v_name"].Value = v_name;
            Dts.Variables["v_description"].Value = v_description;
            Dts.Variables["v_smsCount"].Value = v_smsCount;
            Dts.Variables["v_messageid"].Value = v_messageid;
            Dts.Variables["v_salida_xml"].Value = salida;





            //Escribe en un archivo txt llamado salida.txt las lines que contienen los datos del xml
            using (StreamWriter sw = File.AppendText("F:\\ProyectosIntegracion\\SMS\\salida_sms_basico_infobip\\salidaInfobip.txt"))

                // QUE SE QUIERE VER 
                sw.WriteLine(v_groupName + ";" + v_groupId + ";" + v_messageid + ";" + v_to + ";" + v_id + ";" + v_sms);


            textReader.Close(); //Cierra la lectura 

            Dts.TaskResult = (int)ScriptResults.Success; //Reporta un resultado exitoso para el paquete



        }

        #region ScriptResults declaration
        /// <summary>
        /// This enum provides a convenient shorthand within the scope of this class for setting the
        /// result of the script.
        /// 
        /// This code was generated automatically.
        /// </summary>
        enum ScriptResults
        {
            Success = Microsoft.SqlServer.Dts.Runtime.DTSExecResult.Success,
            Failure = Microsoft.SqlServer.Dts.Runtime.DTSExecResult.Failure
        };
        #endregion

    }
}]]></ProjectItem>
