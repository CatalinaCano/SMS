        #endregion


        /// <summary>
        /// This method is called when this script task executes in the control flow.
        /// Before returning from this method, set the value of Dts.TaskResult to indicate success or failure.
        /// To open Help, press F1.
        /// </summary>
        public void Main()
        {
            // TODO: Add your code here

            string url = Dts.Variables["User::v_url_get"].Value.ToString();
            String texto;
            String textoXml;
            String salida = "";
            String v_groupName = ""; //Equivalente a v_action
            String v_groupId = ""; // Equivalente a  v_statuscode
            String v_messageid = ""; //Mismo v_v_messageid
            String v_date = ""; // Fecha en la que fue recibido doneAt
            int i = 0;
            String authInfo = "ComputecCol:C0mputec2018*";
            authInfo = Convert.ToBase64String(Encoding.Default.GetBytes(authInfo));




            WebRequest request = HttpWebRequest.Create(url);
            request.Method = "GET";
            request.ContentType = "application/xml";
            request.Headers.Add("Authorization", "Basic " + authInfo);


            WebResponse response = request.GetResponse();

            StreamReader reader = new StreamReader(response.GetResponseStream());

            string urlText = reader.ReadToEnd();
            string xml = urlText.Replace("<?xml version='1.0' encoding='utf-8'?>", "");
            /// Escribe el archivo      
            
            using (StreamWriter sw = File.AppendText("F:\\ProyectosIntegracion\\SMS\\salida_get_infobip\\salidaXmlInfobip.xml"))
                sw.WriteLine(urlText);


            // carga el archivo
            XmlTextReader textReader = new XmlTextReader("F:\\ProyectosIntegracion\\SMS\\salida_get_infobip\\salidaXmlInfobip.xml");
            textReader.Read();

            // recorre el archivo           
            while (textReader.Read() && textReader.Value.ToString() != "\r\n")
            {
                XmlNodeType nType = textReader.NodeType;

                if (nType == XmlNodeType.Text)
                {
                    i = i + 1;
                    texto = textReader.Value.ToString() + "|";
                    textoXml = textReader.Value.ToString(); // Almacena el valor de la etiqueta
                    salida += texto;
                    if (i == 1) { v_messageid = textoXml; }
                    if (i == 6) { v_date = textoXml; }
                    if (i == 11) { v_groupId = textoXml; }
                    if (i == 12) { v_groupName = textoXml; }
                }
            }

            if (string.IsNullOrEmpty(v_date))
            {
                v_date = "";
                Dts.Variables["v_groupName"].Value = v_groupName;
                Dts.Variables["v_groupId"].Value = "0";
                Dts.Variables["v_messageid"].Value = v_messageid;
                Dts.Variables["v_date"].Value = DateTime.Now.ToString("yyyy/M/d");
                Dts.Variables["v_salida_xml"].Value = salida;
            }
            else
            {
                v_date = v_date.Substring(0, 10);
                Dts.Variables["v_groupName"].Value = v_groupName;
                Dts.Variables["v_groupId"].Value = v_groupId;
                Dts.Variables["v_messageid"].Value = v_messageid;
                Dts.Variables["v_date"].Value = v_date;
                Dts.Variables["v_salida_xml"].Value = salida;
            }
            using (StreamWriter sw = File.AppendText("F:\\ProyectosIntegracion\\SMS\\salida_get_infobip\\salidaInfobip.txt"))
                //sw.WriteLine(v_action + ";" + v_statuscode + ";" + v_messageid + ";" + v_date);
                sw.WriteLine(v_groupName + ";" + v_groupId + ";" + v_messageid + ";" + v_date);
            textReader.Close();

            Dts.TaskResult = (int)ScriptResults.Success;



        }
    }
    #region ScriptResults declaration
